<rem:section
 oid = "SEC-0008">
<rem:name>
Introducción sobre el dominio del problema</rem:name>
<rem:version>
<rem:major>2</rem:major>
<rem:minor>1</rem:minor>
<rem:date>
<rem:year>2019</rem:year>
<rem:month>10</rem:month>
<rem:day>25</rem:day>
